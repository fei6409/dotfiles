[core]
  autocrlf = false
  filemode = false
  pager = diff-so-fancy | less --tabs=4 -RFX
  excludesfile = /usr/local/google/home/fshao/.gitignore.global
[color]
  ui = true
[alias]
  au = add -u
  bl = blame
  br = branch
  ci = commit
  cia = commit --amend
  co = checkout
  cp = cherry-pick
  dh = diff HEAD
  dhh = diff HEAD^
  grep = "grep -Iin"
  lg = log --stat
  re = rebase
  st = status
  ancestor = !zsh -c 'diff --old-line-format= --new-line-format= <(git rev-list --first-parent \"${1:-master}\") <(git rev-list --first-parent \"${2:-HEAD}\") | head -1' -
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[color "diff"]
  meta = yellow
  frag = magenta bold
  commit = yellow bold
  old = red bold
  new = green bold
  whitespace = red reverse
[pull]
  rebase = false
[include]
  path = ~/.gitconfig.local
